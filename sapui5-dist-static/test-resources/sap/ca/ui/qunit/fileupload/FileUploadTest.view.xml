<core:View xmlns:core="sap.ui.core"
           xmlns="sap.m" xmlns:ui="sap.ca.ui" controllerName="fileupload.FileUploadTest">
    <Page id="page">
        <content>
            <ui:FileUpload
                id="fileupload"
                items="/dataitems"
                uploadUrl="upload"
                fileName="filename"
                size="size"
                url="url"
                uploadedDate="uploaded"
                contributor="contributor"
                fileExtension="extension"
                deleteFile="onDeleteFile" >
            </ui:FileUpload>
        </content>
        <footer>
            <Bar>
            </Bar>
        </footer>
    </Page>
</core:View>
